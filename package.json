{
  "name": "vscode-screenshots",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "publisher": "Gil Fink",
  "activationEvents": [
    "onCommand:applitools-webview.start"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "applitools-webview.start",
        "title": "Applitools Screenshots"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Applitools configurations",
      "properties": {
        "applitoolsUnderTestUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "Url in which you want to create screenshots when you run the Applitools Screenshots WebView."
        },
        "applitoolsAPIKey": {
          "type": "string",
          "default": "",
          "description": "The Applitools API key to run the screenshots."
        },
        "applitoolsProxy": {
          "type": "string",
          "default": "undefined",
          "description": "The Applitools Proxy Url key to use."
        },
        "applitoolsServerUrl": {
          "type": "string",
          "default": "https://eyes.applitools.com",
          "description": "The Applitools server Url key to use."
        },
        "applitoolsSizeMode": {
          "type": "string",
          "default": "full-page",
          "description": "The size mode to use in screenshots. Can be either full-page or viewport."
        },
        "applitoolsBrowsers": {
          "type": "object",
          "default": {
            "browsers": [
              {
                "name": "chrome",
                "width": 3840,
                "height": 2160
              }
            ]
          },
          "description": "The browsers to use in the screenshots webview."
        }
      }
    }
  },
  "dependencies": {
    "@applitools/dom-snapshot": "^1.0.7",
    "@applitools/visual-grid-client": "^12.6.0",
    "prop-types": "^15.7.2",
    "puppeteer": "^1.12.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "2.16.0",
    "react-spinners": "^0.5.4",
    "vscode": "^1.1.17",
    "ws": "^6.2.1"
  },
  "scripts": {
    "vscode:prepublish": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build && tsc -p tsconfig.extension.json",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.2",
    "typescript": "^2.8.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5"
  }
}
